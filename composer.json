{
    "name": "aleron75/mage2hints",
    "description": "A Magento 2 extension that provides some useful debug information while developing",
    "type": "magento2-module",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Alessandro Ronchi",
            "email": "aleron75@gmail.com"
        }
    ],
    "require": {
        "php": "~5.5.0|~5.6.0|~7.0.0|~7.1.0|~7.2.0",
        "magento/framework": "~100.0|~101.0"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable"
    },
    "repositories": [
        {
            "type": "composer",
